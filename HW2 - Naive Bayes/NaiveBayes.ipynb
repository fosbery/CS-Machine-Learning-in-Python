    "# now we can define a function to take an example and\n",
    "# classify it using Naive Bayes (note that we treat the occurence counts as globals)\n",
    "# NOTE: this function should return a class label\n",
    "def naiveBayesClassify(example):\n",
    "    repprob = labelProbs['republican']\n",
    "    demprob = labelProbs['democrat']\n",
    "    colcounter = 0\n",
    "    for col in example:\n",
    "        for key,value in conditionalProbs.items():\n",
    "            if key[1] == colcounter and col == key[2] and key[0]=='democrat':\n",
    "                demprob *= value\n",
    "            elif key[1] == colcounter and col == key[2] and key[0]=='republican':\n",
    "                repprob *= value\n",
    "        colcounter += 1\n",
    "    if demprob > repprob:\n",
    "        return 'democrat'\n",
    "    elif repprob > demprob:\n",
    "        return 'republican'"
   ]
